class Solution {
public:
    vector<int> getRow(int rowIndex) {
        vector<int>vec(rowIndex+1);
        long long res=1;
        
        vec[0]=vec[rowIndex]=1;
        for(int i=1;i<=rowIndex;i++){
            res=res*(rowIndex-i+1);
            res/=i;
            vec[i]=res;
        }
        return vec;
    }
};
