class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int maxim=INT_MIN;
        int count=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]==1){
                count++;
            }
            else{
                count=0;
            }
            maxim=max(maxim,count);
        }
        return maxim;
    }
};
