class Solution {
public:
    vector<int> constructRectangle(int area) {
    //     int l=sqrt(area);
    //     int w=sqrt(area);
    //     while(l*w!=area){
    //         if(l*w<area) l++;
    //         else w--;
    //     }
    //     return {l,w};
        
         vector<int>ans(2);
        for(int i=1;i*i<=area;i++){
            if(area%i==0){
                ans[1]=i;
            }
        }
        ans[0]=area/ans[1];
        return ans;
        
    }
};
