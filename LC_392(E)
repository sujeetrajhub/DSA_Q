class Solution {
public:
    char findTheDifference(string s, string t) {
        // int ans=0;
        // //for(auto i : s) ans = ans xor i;
        //  // for(auto i: t) ans = ans xor i;
        // for(int x:s+t){
        //     ans=ans^x;
        // }
        // return ans;
        
        int n=s.size();
        sort(s.begin(),s.end());
        sort(t.begin(),t.end());
        
        for(int i=0;i<n;i++){
            if(s[i]!=t[i]) return t[i];
        }
        return t[n];
    }
};
