class Solution {
public:
    int lengthOfLastWord(string s) {
        int n =s.length()-1;
        int count=0;
        
        while(s[n]==' '){
            n--;
        }
        while(n>=0 && s[n]!=' '){
            count++;
            n--;
        }
        return count;
    }
};


stringstream str(s);
        string word;
        int count=0;
        while(str>>word) count=word.length();
        return count;
