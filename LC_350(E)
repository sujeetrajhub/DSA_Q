class Solution {
public:
    vector<int> intersect(vector<int>& nums1, vector<int>& nums2) {
        vector<int>vec;
        
        for(auto it:nums1){
            auto it2=find(nums2.begin(),nums2.end(),it);
            if(it2!=nums2.end()){
                vec.push_back(it);
                nums2.erase(it2);
            }
        }
        return vec;
    }
};
